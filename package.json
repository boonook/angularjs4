    "npm-run-all": "^4.1.1",
    "protractor": "~5.1.2",
    "stubby": "^4.0.0",
    "sw-precache": "^5.2.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typedoc": "^0.8.0",
    "typescript": ">=2.4.2 <2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://git.oschina.net/greengerong/rebirth.git"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
